<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <update handle="default_account_manager_layout"/>

    <head>
        <title>Dashboard | Manager Account</title>
    </head>

    <!-- Set page title: -->
    <referenceBlock name="page.main.title">
        <action method="setPageTitle">
            <argument translate="true" name="title" xsi:type="string">Dashboard</argument>
        </action>
    </referenceBlock>

    <referenceContainer name="manager.main">
        <!-- Display Manager Account Dashboard-->
        <block name="manager.dashboard" cacheable="false" template="BelSmol_VideoCall::manager/dashboard.phtml" />
    </referenceContainer>
</page>
